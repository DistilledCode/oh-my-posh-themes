{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        // {
        //   "foreground": "lightYellow",
        //   "style": "plain",
        //   "template": "{{ .UserName }}: ",
        //   "type": "session"
        // },
        {
          "foreground": "#f6df10",
          "properties": {
            "fetch_version": true
          },
          "style": "plain",
          "template": "(\ue235  {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}) ",
          "type": "python"
        },
        {
          "foreground": "#04ff00",
          "properties": {
            "style": "folder"
          },
          "style": "plain",
          "template": "{{ .Path }} ",
          "type": "path"
        },
        {
          "foreground": "#ff94df",
          "properties": {
            "branch_icon": " <#ff94df><b>\ue0a0 </b></>",
            "branch_max_length": 25,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_stash_count": true
          },
          "style": "plain",
          "template": "<#ffffff>on</>{{ .UpstreamIcon }}{{ .HEAD }}{{ .BranchStatus }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \uf692 {{ .StashCount }}{{ end }} ",
          "type": "git"
        }
      ],
      "type": "prompt"
    },

    {
      "alignment": "right",
      "segments": [
        {
          "foreground": "#AEA4BF",
          "properties": {
            "style": "austin",
            "threshold": 150
          },
          "style": "plain",
          "template": "{{ .FormattedMs }} <#ffffff>|</>",
          "type": "executiontime"
        },
        {
          "foreground": "#00C5C7",
          "properties": {
            "time_format": "15:04:05"
          },
          "style": "plain",
          "template": " {{ .CurrentDate | date .Format }} ",
          "type": "time"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "style": "plain",
          "template": "$",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}
